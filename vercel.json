{
  "functions": {
    "api/llm/route.js": {
      "runtime": "edge"
    },
    "api/llm/health.js": {
      "runtime": "edge"
    },
    "api/llm/agents.js": {
      "runtime": "edge"
    },
    "api/dns/verification.js": {
      "runtime": "edge"
    },
    "api/deployment/dnsReadinessCheck.js": {
      "runtime": "edge"
    },
    "api/monitoring/errors.js": {
      "runtime": "edge"
    }
  },
  "env": {
    "GROQ_API_KEY_1": "@groq-api-key-1",
    "GROQ_API_KEY_2": "@groq-api-key-2",
    "GROQ_API_KEY_3": "@groq-api-key-3",
    "CEREBRAS_API_KEY_1": "@cerebras-api-key-1",
    "CEREBRAS_API_KEY_2": "@cerebras-api-key-2",
    "CEREBRAS_API_KEY_3": "@cerebras-api-key-3",
    "GOOGLE_API_KEY_1": "@google-api-key-1",
    "GOOGLE_API_KEY_2": "@google-api-key-2",
    "GOOGLE_API_KEY_3": "@google-api-key-3",
    "LANGSMITH_API_KEY": "@langsmith-api-key",
    "LANGSMITH_TRACE_URL": "@langsmith-trace-url",
    "DNS_VERIFICATION_TIMEOUT": "30000",
    "HEALTH_CHECK_INTERVAL": "300000",
    "PROVIDER_CACHE_TTL": "300000",
    "MAX_RETRIES": "3",
    "FALLBACK_TIMEOUT": "10000"
  },
  "build": {
    "env": {
      "ENABLE_EXPERIMENTAL_COREPACK": "1"
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" }
      ]
    }
  ],
  "regions": ["iad1", "sfo1"],
  "framework": "vite"
}
