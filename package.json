{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "node dev-server.mjs",
    "dev:full": "concurrently \"npm run dev:api\" \"npm run dev\"",
    "build": "vite build",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --config vitest.config.ts",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:integration:e2e": "vitest run --config vitest.integration.config.ts tests/integration/workflow-e2e.test.ts",
    "test:integration:api": "vitest run --config vitest.integration.config.ts tests/integration/workflow-api.test.ts",
    "test:integration:performance": "vitest run --config vitest.integration.config.ts tests/integration/workflow-performance.test.ts",
    "test:load": "vitest run --config vitest.integration.config.ts --reporter=json --outputFile=test-results/load-test-results.json",
    "test:performance": "vitest run --config vitest.performance.config.ts",
    "test:performance:budget": "node scripts/performance-budget-check.js",
    "test:performance:baseline": "node scripts/create-performance-baseline.js",
    "test:performance:compare": "node scripts/compare-performance.js",
    "test:report": "node scripts/generate-test-report.js",
    "test:report:pr": "node scripts/generate-pr-comment.js",
    "test:aggregate": "node scripts/aggregate-test-results.js",
    "test:dashboard": "node scripts/generate-test-dashboard.js",
    "preview:performance": "vite preview --port 4173 --host 0.0.0.0",
    "health:check": "node scripts/health-check.js",
    "compare:performance": "node scripts/compare-performance.js",
    "validate:deployment": "npm run type-check && npm run build && npm run health:check",
    "deploy": "vercel",
    "deploy:prod": "vercel --prod",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@cerebras/cerebras_cloud_sdk": "^1.50.0",
    "@google/generative-ai": "^0.24.1",
    "@hookform/resolvers": "^5.2.2",
    "@langchain/community": "^0.3.56",
    "@langchain/core": "^0.3.77",
    "@langchain/google-genai": "^0.2.18",
    "@langchain/groq": "^0.2.4",
    "@langchain/langgraph": "^0.4.9",
    "@tavily/core": "^0.5.12",
    "@types/node": "^24.5.2",
    "@upstash/qstash": "^2.8.3",
    "@upstash/redis": "^1.35.4",
    "@upstash/vector": "^1.2.2",
    "dotenv": "^17.2.2",
    "groq-sdk": "^0.30.0",
    "langsmith": "^0.3.69",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "uuid": "^13.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@edge-runtime/vm": "^5.0.0",
    "@eslint/js": "^9.9.1",
    "@faker-js/faker": "^10.0.0",
    "@octokit/rest": "^20.0.2",
    "@playwright/test": "^1.55.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "ajv": "^8.17.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "express": "^5.1.0",
    "globals": "^15.9.0",
    "jest-environment-jsdom": "^30.1.2",
    "jest-openapi": "^0.14.2",
    "js-yaml": "^4.1.0",
    "jsdom": "^27.0.0",
    "msw": "^2.11.2",
    "playwright": "^1.55.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  }
}
