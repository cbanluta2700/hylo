# Test Environment Configuration for Hylo AI Workflow System
# This file contains environment variables for integration and e2e testing

# =============================================================================
# API Configuration
# =============================================================================

# Base API URL for testing (will be overridden in CI/CD)
VITE_API_BASE_URL=http://localhost:3000/api
VERCEL_URL=localhost:3000

# Test timeout configurations
VITE_TEST_TIMEOUT=30000
VITE_INTEGRATION_TEST_TIMEOUT=60000
VITE_PERFORMANCE_TEST_TIMEOUT=120000

# =============================================================================
# LLM Provider Configuration (Test Mode)
# =============================================================================

# Groq Configuration for testing
GROQ_API_KEY=test_groq_api_key_replace_in_ci
GROQ_MODEL_COMPOUND=llama-3.1-70b-versatile
GROQ_MODEL_FALLBACK=mixtral-8x7b-32768
GROQ_MAX_TOKENS=4096
GROQ_TEMPERATURE=0.1

# Cerebras Configuration for testing
CEREBRAS_API_KEY=test_cerebras_api_key_replace_in_ci
CEREBRAS_MODEL_PRIMARY=llama3.1-70b
CEREBRAS_MODEL_FALLBACK=llama3.1-8b
CEREBRAS_MAX_TOKENS=2048
CEREBRAS_TEMPERATURE=0.2

# Google Gemini Configuration for testing
GOOGLE_API_KEY=test_google_api_key_replace_in_ci
GOOGLE_MODEL_PRIMARY=gemini-1.5-pro
GOOGLE_MODEL_FALLBACK=gemini-1.5-flash
GOOGLE_MAX_TOKENS=8192
GOOGLE_TEMPERATURE=0.3

# =============================================================================
# Database Configuration (Test)
# =============================================================================

# Upstash Vector Database for testing
UPSTASH_VECTOR_REST_URL=https://test-vector-db.upstash.io
UPSTASH_VECTOR_REST_TOKEN=test_upstash_vector_token_replace_in_ci

# Upstash Redis for session management
UPSTASH_REDIS_REST_URL=https://test-redis.upstash.io
UPSTASH_REDIS_REST_TOKEN=test_upstash_redis_token_replace_in_ci

# QStash for background job processing
QSTASH_URL=https://qstash.upstash.io
QSTASH_TOKEN=test_qstash_token_replace_in_ci

# =============================================================================
# Vector Database & Embeddings
# =============================================================================

# Jina Embeddings Configuration
JINA_API_KEY=test_jina_api_key_replace_in_ci
JINA_EMBEDDING_MODEL=jina-embeddings-v2-base-en
JINA_EMBEDDING_DIMENSIONS=768

# Alternative: Qdrant Configuration for local testing
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=test_qdrant_key_optional

# =============================================================================
# Observability & Monitoring
# =============================================================================

# LangSmith Configuration for testing
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=test_langsmith_api_key_replace_in_ci
LANGCHAIN_PROJECT=hylo-ai-workflow-test
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com

# Custom monitoring endpoints
MONITORING_WEBHOOK_URL=https://test-monitoring.example.com/webhook
ALERT_EMAIL=test@example.com

# =============================================================================
# Cost & Resource Limits (Test Environment)
# =============================================================================

# Workflow execution limits for testing
MAX_WORKFLOW_COST=5.00
MAX_WORKFLOW_EXECUTION_TIME=60000
MAX_CONCURRENT_WORKFLOWS=10
MAX_MEMORY_USAGE_MB=500

# Agent-specific cost limits
CONTENT_PLANNER_MAX_COST=0.50
INFO_GATHERER_MAX_COST=2.00
STRATEGIST_MAX_COST=1.00
COMPILER_MAX_COST=1.50

# =============================================================================
# External Services (Test/Mock)
# =============================================================================

# Web search and content retrieval
JINA_READER_API_KEY=test_jina_reader_key_replace_in_ci
SERP_API_KEY=test_serp_api_key_replace_in_ci

# Rate limiting configuration
RATE_LIMIT_REQUESTS_PER_MINUTE=100
RATE_LIMIT_COST_PER_HOUR=10.00

# =============================================================================
# Test-Specific Configuration
# =============================================================================

# Enable/disable test features
ENABLE_MOCK_AGENTS=false
ENABLE_COST_TRACKING=true
ENABLE_PERFORMANCE_MONITORING=true
ENABLE_ERROR_SIMULATION=false

# Test data configuration
TEST_DATA_PATH=./tests/fixtures
MOCK_RESPONSE_DELAY=100

# Cleanup configuration
AUTO_CLEANUP_TEST_SESSIONS=true
TEST_SESSION_TTL=3600

# =============================================================================
# CI/CD Configuration
# =============================================================================

# GitHub Actions integration
CI=false
GITHUB_ACTIONS=false
GITHUB_TOKEN=replace_in_ci

# Test reporting
JUNIT_OUTPUT_PATH=./test-results/junit.xml
COVERAGE_OUTPUT_PATH=./coverage
PERFORMANCE_REPORT_PATH=./test-results/performance.json

# Deployment configuration
DEPLOY_ENVIRONMENT=test
VERCEL_TOKEN=replace_in_ci
VERCEL_ORG_ID=replace_in_ci
VERCEL_PROJECT_ID=replace_in_ci

# =============================================================================
# Security Configuration
# =============================================================================

# CORS configuration for testing
CORS_ORIGIN=http://localhost:3000,http://localhost:3001
CORS_CREDENTIALS=true

# API security
API_RATE_LIMIT=1000
API_CORS_ENABLED=true
API_AUTH_REQUIRED=false

# Session security
SESSION_SECRET=test_session_secret_replace_in_production
JWT_SECRET=test_jwt_secret_replace_in_production
JWT_EXPIRY=3600

# =============================================================================
# Development & Debugging
# =============================================================================

# Logging configuration
LOG_LEVEL=debug
LOG_FORMAT=json
LOG_OUTPUT=console

# Debug flags
DEBUG_AGENT_EXECUTION=true
DEBUG_WORKFLOW_STATE=true
DEBUG_COST_CALCULATION=true
DEBUG_PERFORMANCE_METRICS=true

# Verbose output for testing
VERBOSE_TESTING=false
TRACE_AGENT_COMMUNICATION=false